{
  "name": "ts",
  "version": "0.0.1",
  "scripts": {
    "prestart": "npm run build",
    "start": "node dist/main",
    "start:dev": "nodemon ./src/main.ts --watch src --exec \"npm run lint:dev && ts-node\"",
    "build": "tsc",
    "lint": "eslint {src,test}/**/*.ts --fix",
    "lint:dev": "eslint {src,test}/**/*.ts --max-warnings 2"
  },
  "engines": {
    "node": ">= 16.14.2",
    "npm": "~7.12.1"
  },
  "license": "ISC",
  "dependencies": {
    "rxjs": "^6.5.4"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "eslint": "^8.20.0",
    "nodemon": "^2.0.15",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  }
}
